{
  "name": "vite-plugin-html-crossorigin",
  "version": "0.1.1",
  "description": "Vite plugin for adding crossorigin attribute to emitted html",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "dev": "vite",
    "css": "tailwindcss -i ./app/input.css -o ./app/output.css --watch",
    "tsc": "tsc --noEmit",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HexMox/vite-plugin-html-crossorigin"
  },
  "homepage": "https://github.com/HexMox/vite-plugin-html-crossorigin",
  "keywords": [
    "vite",
    "vite plugin",
    "crossorigin"
  ],
  "author": "moxhe",
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/cheerio": "^0.22.31",
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12"
  }
}
